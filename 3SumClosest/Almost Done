class Solution:
    def threeSumClosest(self, nums: List[int], target: int) -> int:
        n=len(nums)
        t= nums[0] + nums[1] + nums[2]
        for i in range(n):
            for j in range (i+1,n):
                for k in range(j+1,n):
                    c = nums[i] + nums[j] + nums[k]
        return c            

        
